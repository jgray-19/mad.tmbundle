{
    "name": "mad-tmlanguage",
    "displayName": "MAD-NG Language Extension",
    "version": "0.0.3",
    "license": "MIT",
    "publisher": "jgray-19",
    "engines": {
        "vscode": "^1.74.0"
    },
    "contributes": {
        "languages": [{
          "id": "mad",
          "aliases": ["mad", "MAD-NG"],
          "extensions": [".mad"],
          "configuration": "./language-configuration.json",
          "icon": {"light": "./icons/icon.png", "dark": "./icons/icon.png"}
          }],
        "grammars": [{
          "language": "mad",
          "scopeName": "source.mad",
          "path": "Syntaxes/mad.tmLanguage.json"
        }]
      },
    "repository": {
      "type": "git",
      "url": "https://github.com/jgray-19/madng-language.git"
    }
}
